{
  "compilerOptions": {
    "strict": true,
    "target": "es2017",
    "module": "commonjs",                  /* Specify module code generation: 'none', 'commonjs', 'amd', 'system', 'umd', 'es2015', or 'ESNext'. */
    "lib": [
      "es2015",
      "es2016",
      "dom"
    ],
    "rootDir": "src",
    "outDir": "dist",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  }
}
